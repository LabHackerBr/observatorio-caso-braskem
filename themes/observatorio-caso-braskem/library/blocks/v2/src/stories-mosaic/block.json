{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hacklabr/stories-mosaic",
	"version": "0.1.26",
	"title": "Mosaic Stories",
	"category": "text",
	"icon": "excerpt-view",
	"description": "Exibe os Ãºltimos posts em formato de mosaico e com slider.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"blockId": {
			"type": "string"
		},
		"blockModel": {
			"type": "string",
			"default": "post"
		},
		"coAuthor": {
			"type": "string"
		},
		"compare": {
			"type": "string",
			"default": "OR"
		},
		"contentBelow": {
			"type": "boolean",
			"default": false
		},
		"columns": {
			"type": "number",
			"default": 2
		},
		"heading": {
			"type": "string"
		},
		"linkUrl": {
			"type": "string"
		},
		"noCompare": {
			"type": "string",
			"default": "OR"
		},
		"noPostType": {
			"type": "string"
		},
		"noQueryTerms": {
			"type": "array"
		},
		"noTaxonomy": {
			"type": "string"
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderBy": {
			"type": "string",
			"default": "date"
		},
		"postsToShow": {
			"type": "number",
			"default": 8
		},
		"postType": {
			"type": "string",
			"default": "post"
		},
		"queryTerms": {
			"type": "array"
		},
		"showAuthor": {
			"type": "boolean",
			"default": true
		},
		"showChildren": {
			"type": "boolean",
			"default": false
		},
		"showDate": {
			"type": "boolean",
			"default": false
		},
		"showExcerpt": {
			"type": "boolean",
			"default": false
		},
		"showThumbnail": {
			"type": "boolean",
			"default": true
		},
		"showTaxonomy": {
			"type": "string",
			"default": "category"
		},
		"taxonomy": {
			"type": "string"
		},
		"thumbnailFormat": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "hacklabr",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
